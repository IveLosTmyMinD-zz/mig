var s, t, v:real;   //Объявляем переменные
begin               //Начало программы
writeln('Vvedite rasstoyanie:');
readln(s);          //Запрос расстояния
writeln('Vvedite vremya:');
readln(t);          //Запрос времени
v:= s/t;            //Формула высчитывания скорости
writeln(v);         //Вывод скорости на экран
end.                //Конец программы

